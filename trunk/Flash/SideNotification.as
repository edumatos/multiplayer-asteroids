package {	import flash.display.MovieClip	import flash.utils.setTimeout	import fl.transitions.Tween; 	import fl.transitions.easing.*;	import fl.transitions.TweenEvent	public class SideNotification extends MovieClip{		private var tween:Tween;		function SideNotification(){			var that:MovieClip = this						tween = new Tween(inner, "x", Strong.easeOut, 0, -inner.width, .8, true);			tween.addEventListener(TweenEvent.MOTION_FINISH, function(e){				setTimeout(function(){					tween = new Tween(inner, "x", Strong.easeIn, inner.x, 0, .8, true);					tween.addEventListener(TweenEvent.MOTION_FINISH, function(e){						that.parent.removeChild(that);						tween = null					})				},2500)			})		}	}}