package {	import flash.display.MovieClip;	import flash.utils.Dictionary;	public class Userlist extends MovieClip{		private var users:Dictionary		public function Userlist(){			users = new Dictionary()		}				public function AddUser(s:Ship):void{						var u:UserlistItem = new UserlistItem(s);			addChild(u)			users[s] = u						Refresh();		}		public function RemoveUser(s:Ship):void{						var u:UserlistItem = users[s] as UserlistItem			if(u){				removeChild(u)				delete users[s]			}						Refresh();					}		public function Refresh():void{			var ta:Array = []			for( var s:Object in users){				var ship:Ship = s as Ship;				var user:UserlistItem = users[s] as UserlistItem								ta.push([ship, user])			}									ta.sort(function(a,b){				var av:Number = a[0].Kills - a[0].Deaths				var bv:Number = b[0].Kills - b[0].Deaths				return av < bv ? 1 : -1			})						for( var a:int=0;a<ta.length;a++){				ta[a][1].Refresh(a+1)				ta[a][1].y = a*15			}						background.height = a*14+10					}	}}